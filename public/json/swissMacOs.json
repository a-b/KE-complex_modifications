{
    "title": "Swiss Keyboard Layout for macOS",
    "rules": [
        {
            "description": "ctrl + c --> copy OR alt-gr + 2 --> @",
            "manipulators": [
                {
                    "from": {
                        "key_code": "left_control"
                    },
                    "to": [
                        {
                            "key_code": "left_command"
                        }
                    ],
                    "type": "basic"
                },
                {
                    "from": {
                        "key_code": "right_control"
                    },
                    "to": [
                        {
                            "key_code": "right_command"
                        }
                    ],
                    "type": "basic"
                },
                {
                    "from": {
                        "key_code": "2",
                        "modifiers": {
                            "mandatory": [
                                "left_option"
                            ],
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "g",
                            "modifiers": [
                                "left_option"
                            ]
                        }
                    ],
                    "type": "basic"
                },
                {
                    "from": {
                        "key_code": "2",
                        "modifiers": {
                            "mandatory": [
                                "right_option"
                            ],
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "g",
                            "modifiers": [
                                "right_option"
                            ]
                        }
                    ],
                    "type": "basic"
                },
                {
                    "from": {
                        "key_code": "quote",
                        "modifiers": {
                            "mandatory": [
                                "left_option"
                            ],
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "8",
                            "modifiers": [
                                "left_option"
                            ]
                        }
                    ],
                    "type": "basic"
                },
                {
                    "from": {
                        "key_code": "non_us_pound",
                        "modifiers": {
                            "mandatory": [
                                "left_option"
                            ],
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "9",
                            "modifiers": [
                                "left_option"
                            ]
                        }
                    ],
                    "type": "basic"
                },
                {
                    "from": {
                        "key_code": "open_bracket",
                        "modifiers": {
                            "mandatory": [
                                "left_option"
                            ],
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "5",
                            "modifiers": [
                                "left_option"
                            ]
                        }
                    ],
                    "type": "basic"
                },
                {
                    "from": {
                        "key_code": "close_bracket",
                        "modifiers": {
                            "mandatory": [
                                "left_option"
                            ],
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "6",
                            "modifiers": [
                                "left_option"
                            ]
                        }
                    ],
                    "type": "basic"
                },
                {
                    "from": {
                        "key_code": "equal_sign",
                        "modifiers": {
                            "mandatory": [
                                "right_option"
                            ],
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "n",
                            "modifiers": [
                                "right_option"
                            ]
                        }
                    ],
                    "type": "basic"
                },
                {
                    "from": {
                        "key_code": "equal_sign",
                        "modifiers": {
                            "mandatory": [
                                "left_option"
                            ],
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "n",
                            "modifiers": [
                                "left_option"
                            ]
                        }
                    ],
                    "type": "basic"
                },
                {
                    "from": {
                        "key_code": "non_us_backslash",
                        "modifiers": {
                            "mandatory": [
                                "left_option"
                            ],
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "7",
                            "modifiers": [
                                "left_shift",
                                "left_option"
                            ]
                        }
                    ],
                    "type": "basic"
                },
                {
                    "from": {
                        "key_code": "non_us_backslash",
                        "modifiers": {
                            "mandatory": [
                                "right_option"
                            ],
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
